@font-face
	font-family 'angytours-andada bold'
	src:url('../fonts/Andada-Bold.otf') format("opentype")

@font-face 
	font-family 'angytours-andada italic'
	src:url('../fonts/Andada-Italic.otf') format("opentype")

@font-face 
	font-family 'angytours-bariol'
	src:url('../fonts/Bariol_Regular.otf') format("opentype")
	
@font-face
	font-family 'angytours-icon'
	src:url('../fonts/icomoon.eot?6e85gn')
	src:url('../fonts/icomoon.eot?#iefix6e85gn') format('embedded-opentype'),
		url('../fonts/icomoon.woff?6e85gn') format('woff'),
		url('../fonts/icomoon.ttf?6e85gn') format('truetype'),
		url('../fonts/icomoon.svg?6e85gn#icomoon') format('svg');
	font-weight normal
	font-style normal

[class^="icon-"], [class*=" icon-"] {
	font-family: 'angytours-icon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

